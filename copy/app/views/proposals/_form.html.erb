<%= form_with(model: proposal) do |form| %>
  <%= render "shared/error_messages", resource: form.object %>
  <div class="form-group">
    <%= form.label :blocked_on %>
    <%= form.select :blocked_on, Proposal.blocked_choices, class: "rounded form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :reason_incomplete %>
    <%= form.text_field :reason_incomplete, class: "form-control" %>
  </div>

  <div class="form-group flex justify-between">
    <%= form.button class: "btn btn-primary" %>
  </div>
<% end %>
