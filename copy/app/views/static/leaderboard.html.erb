                    <div class="flex flex-row items-center">
                      <div class="px-4 py-2 text-m font-medium border shadow rounded-full text-green-600 bg-green-100 border-green-400 hidden">
                        <%#= @job_position.active ? 'Active' : 'Inactive' %>
                      </div>
                    </div>
                  </div>
                </div>
